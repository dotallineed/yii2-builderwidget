{
    "name": "dotallineed/yii2-widgets-builder",
    "description": "An extension for creating and storing html widgets with content to the database for Yii2 site builder",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "homepage": "https://github.com/dotallineed/yii2-widgets-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Kinggeorge Magaga",
            "email": "magagageorge@dotallineed.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dotallineed/yii2-widgets-builder.git"
        }
    ],
    
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "dotallineed\\builderwidget\\": ""
        }
    }
